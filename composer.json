{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.8",
        "ext-redis": "*",
        "RobBrazier/Piwik": "^3.0",
        "andreasindal/laravel-markdown": "^1.1",
        "cviebrock/eloquent-taggable": "^2.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "douyasi/laravel-editor-md": "dev-master",
        "fideloper/proxy": "~3.3",
        "greggilbert/recaptcha": "^2.2",
        "hootlex/laravel-moderation": "^1.0",
        "intervention/image": "^2.3",
        "jacobbennett/laravel-http2serverpush": "^2.0",
        "jcc/laravel-vote": "^1.1",
        "jenssegers/agent": "^2.5",
        "junaidnasir/larainvite": "^0.1.9",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^2.0",
        "laravel/tinker": "~1.0",
        "league/html-to-markdown": "^4.4",
        "lincanbin/material-design-avatars": "^1.3",
        "overtrue/laravel-follow": "^1.1",
        "overtrue/laravel-lang": "^3.0",
        "overtrue/laravel-socialite": "^1.0",
        "overtrue/laravel-ueditor": "^1.2",
        "predis/predis": "^1.1",
        "ricoa/copywriting-correct": "^0.1.2",
        "schuppo/password-strength": "^1.9",
        "spatie/laravel-pjax": "^1.3",
        "tom-lingham/searchy": "^2.0",
        "vi-kon/laravel-diff": "^1.0",
        "wearejust/laravel-amp": "dev-master",
        "zizaco/entrust": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
