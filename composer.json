{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.8",
        "ext-redis": "*",
        "RobBrazier/Piwik": "^2.1",
        "andreasindal/laravel-markdown": "^1.1",
        "cviebrock/eloquent-taggable": "^2.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "douyasi/laravel-editor-md": "dev-master",
        "ghanem/rating": "^1.4",
        "greggilbert/recaptcha": "dev-master",
        "hootlex/laravel-moderation": "^1.0",
        "intervention/image": "^2.3",
        "jcc/laravel-vote": "^1.0",
        "jenssegers/agent": "^2.5",
        "junaidnasir/larainvite": "^0.1.7",
        "laracasts/flash": "^2.0",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^2.0",
        "laravel/tinker": "~1.0",
        "league/html-to-markdown": "^4.4",
        "lincanbin/material-design-avatars": "^1.3",
        "overtrue/laravel-follow": "^1.0",
        "overtrue/laravel-lang": "^3.0",
        "overtrue/laravel-ueditor": "~1.0",
        "overtrue/laravel-socialite": "~1.0",
        "predis/predis": "^1.1",
        "ricoa/copywriting-correct": "dev-master",
        "schuppo/password-strength": "~1.5",
        "spatie/laravel-pjax": "^1.3",
        "tom-lingham/searchy": "^2.0",
        "vi-kon/laravel-diff": "^1.0",
        "wearejust/laravel-amp": "dev-master",
        "zizaco/entrust": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
